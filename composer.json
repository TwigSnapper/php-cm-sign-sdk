    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "CmSignSdk": "src/",
            "CmSignSdk\\Tests": "tests/"
        }
    },
    "authors": [
        {
            "name": "Chris Smits",
            "email": "info@chrissmits.nl",
            "homepage": "https://github.com/chrissmits91/php-cm-sign-sdk",
